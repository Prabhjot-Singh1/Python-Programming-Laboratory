    "# Sample dictionary\n",
    "my_dict = {'apple': 10, 'banana': 2, 'cherry': 5, 'date': 7}\n",
    "\n",
    "# Sort dictionary by values\n",
    "sorted_dict = dict(sorted(my_dict.items(), key=lambda x: x[1]))\n",
    "\n",
    "print(\"Original dictionary:\", my_dict)\n",
    "print(\"Dictionary sorted by value:\", sorted_dict)\n"
